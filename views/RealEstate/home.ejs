<!-- <p>this is a home page</p> -->

<div class="row">
    <% estateCell.forEach( function(model) {  %>
    <div class="card col-12 col-md-6 homeMain">
        <a href="/estate/detail?id=<%= model.id %>" style="font-size:20px; font-weight: bold;"><%= model.title %></a>
        <div class="row">
            <div class="col-md-3"  style="display: flex; justify-content: center; align-items: center;">
                <img src="<%= model.imageURL %>" class="card-img" alt="...">
            </div>
            <div class="col-md-9">
                <div class="card-body">
                    <ul class="list-group">
                        <li class="list-group-item">Estate: <%= model.estate %></li>
                        <li class="list-group-item">Gross Area: <%= model.grossArea %></li>
                        <li class="list-group-item">Rent: $<%= model.rent %></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <% }) %>

</div>